{
	"name": "flarum-lang/burmese",
	"type": "flarum-extension",
	"description": "Burmese language pack for Flarum.",
	"homepage": "https://github.com/flarum-lang/burmese",
	"keywords": [
		"locale",
		"flarum",
		"flarum-locale",
		"my-locale",
		"burmese-locale",
		"burmese"
	],
	"license": "MIT",
	"support": {
		"issues": "https://github.com/flarum-lang/burmese/issues",
		"source": "https://github.com/flarum-lang/burmese"
	},
	"require": {
		"flarum/core": "^1.6"
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
	"extra": {
		"flarum-extension": {
			"title": "Burmese",
			"category": "language",
			"icon": {
				"image": "icon.svg",
				"backgroundSize": "cover",
				"backgroundPosition": "center"
			}
		},
		"flarum-locale": {
			"code": "my",
			"title": "Burmese"
		},
		"extiverse": {
			"discuss": ""
		}
	}
}
